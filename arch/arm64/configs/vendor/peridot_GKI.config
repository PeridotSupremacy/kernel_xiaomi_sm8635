# Charger
CONFIG_MI_DTPT=y
CONFIG_MI_ENABLE_DP=y

# Debug
CONFIG_MI_DUMP_DISPLAY=m
CONFIG_MTD=m
CONFIG_MTD_BLOCK=m
CONFIG_MTD_BLOCK2MTD=m
CONFIG_MTD_OOPS=m
CONFIG_QCOM_DYN_MINIDUMP_STACK=y
CONFIG_REGMAP_QTI_DEBUGFS_ALLOW_WRITE=y
CONFIG_XLOGCHAR=m

# Fingerprint
CONFIG_FINGERPRINT_MI_FP=m
CONFIG_INPUT_FINGERPRINT=y

# HWID
CONFIG_MI_HARDWARE_ID=m

# IR
CONFIG_IR_SPI=m
CONFIG_LIRC=y
CONFIG_RC_CORE=y
CONFIG_RC_DECODERS=y
CONFIG_RC_DEVICES=y

# Joystick
CONFIG_JOYSTICK_XPAD_FF=y
CONFIG_JOYSTICK_XPAD_LEDS=y

# Net
CONFIG_NETFILTER_XT_MATCH_CONNBYTES=y

# Regulators
CONFIG_REGULATOR_WL2866D=m

# Thermal
CONFIG_MI_THERMAL_INTERFACE=m
CONFIG_MI_THERMAL_MULTI_CHARGE=y

# Vibrator
# CONFIG_LEDS_QPNP_VIBRATOR_LDO is not set
CONFIG_INPUT_AW86927_HAPTIC=m
CONFIG_SIH_VIBRATOR=m
